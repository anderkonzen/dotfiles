[
  {
    "context": "Pane",
    "bindings": {
      "cmd-f1": "pane::GoBack",
      "cmd-f4": "pane::GoForward",
      "ctrl-a |": "pane::SplitRight"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-a ctrl-h": ["workspace::ActivatePaneInDirection", "Left"],
      "ctrl-a ctrl-l": ["workspace::ActivatePaneInDirection", "Right"],
      "ctrl-a ctrl-j": ["workspace::ActivatePaneInDirection", "Up"],
      "ctrl-a ctrl-k": ["workspace::ActivatePaneInDirection", "Down"],
      "ctrl-a q": "workspace::ClearAllNotifications"
    }
  },
  {
    "bindings": {
      "ctrl-t": "tab_switcher::ToggleAll"
    }
  }
]
