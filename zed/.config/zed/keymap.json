[
  {
    "context": "Pane",
    "bindings": {
      "cmd-f1": "pane::GoBack",
      "cmd-f4": "pane::GoForward",
      "ctrl-a |": "pane::SplitRight",
    },
  },
  {
    "context": "Workspace",
    "bindings": {
      "ctrl-a ctrl-h": "workspace::ActivatePaneLeft",
      "ctrl-a ctrl-l": "workspace::ActivatePaneRight",
      "ctrl-a ctrl-j": "workspace::ActivatePaneUp",
      "ctrl-a ctrl-k": "workspace::ActivatePaneDown",
      "ctrl-a q": "workspace::ClearAllNotifications",
    },
  },
  {
    "context": "Editor",
    "bindings": {
      "ctrl-a g": "editor::OpenPermalinkToLine",
    },
  },
]
